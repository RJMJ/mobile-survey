.survey-admin-nav
  float right
  .active
    background lightgray

#toggleTrigger
  position relative
  z-index 2

#map
  height 400px
  width 100%

.nav-floating-row
  bottom 0
  position relative
  z-index 2

.tab-content-floating-row
  position relative
  z-index 2

.map-floating-row
  position relative
  z-index 1
  margin-left -10%
  margin-right -10%
  margin-bottom -110px
  top -90px

.buttons-floating-row
  position relative
  z-index 1

.trigger-tabs
  margin-left -15px
  margin-right -15px

.trigger-nav-small > li > a
  padding 5px 10px
  border none
  border-radius 0

.trigger-nav-small > li > a:hover
  border none

.trigger-nav-small > li > a:focus
  border none

.trigger-nav-small > li.active > a
  border none
  width 100%

.trigger-nav-small > li.active > a:hover
  border none

.trigger-nav-small > li.active > a:focus
  border none

.trigger-nav-small > li
  background-color #eee
  border thin solid #333
  margin-right 1px

.trigger-nav-small
  margin-bottom 5px
  border-bottom none
